{
  "name": "homebridge-onstar",
  "version": "1.5.6",
  "description": "Unofficial OnStar plugin for Homebridge",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "config.schema.json"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rm -rf dist && rollup -c -w",
    "lint": "prettier --write .",
    "prepublishOnly": "yarn run test && yarn run build",
    "prerelease": "yarn run test && yarn run build",
    "release": "standard-version --sign",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samrum/homebridge-onstar.git"
  },
  "keywords": [
    "homebridge-plugin",
    "onstar",
    "chevrolet",
    "chevy",
    "gm",
    "gmc",
    "buick",
    "cadillac"
  ],
  "author": "Ruben Medina <dev@rubenmedina.com> (https://rubenmedina.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samrum/homebridge-onstar/issues"
  },
  "engines": {
    "node": ">=11.0.0",
    "homebridge": ">=0.4.0"
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/samrum"
  },
  "homepage": "https://github.com/samrum/homebridge-onstar#readme",
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "onstarjs": "^2.0.6"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "prettier": "2.3.2",
    "rollup": "^2.45.2",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "ts-mockito": "^2.3.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,css,md,json}": "prettier --write"
  }
}
